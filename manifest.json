{
    "name" : "LaftelSync",
    "description": "Sync Laftel Anime & watch Anime with friends",
    "version" : "1.0",
    "manifest_version": 3,

    "action":{
        "default_title": "testing",
        "default_icon":"icon.png",
        "default_popup":"index.html"
    },
    "background": {
        "service_worker" : "js/background.js"
    },
    "permissions":[
        "alarms",
        "tabs"
    ]
}